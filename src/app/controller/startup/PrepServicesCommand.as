package app.controller.startup{	import app.services.FacebookService;	import app.services.InitialDataService;	import app.services.SWFAddressService;	import org.robotlegs.base.ContextEvent;	import org.robotlegs.mvcs.Command;	public class PrepServicesCommand extends Command	{		/*******************************************************************************************		*								public properties										   *		*******************************************************************************************/		[Inject] public var event : ContextEvent;						/*******************************************************************************************		*								public methods											   *		*******************************************************************************************/		public function PrepServicesCommand()		{		}		override public function execute() : void		{			injector.mapValue(SWFAddressService, injector.instantiate(SWFAddressService));			injector.mapSingleton(InitialDataService);			injector.mapSingleton(FacebookService);		}	}}