package app.controller.fb{	import app.models.fb.FBModel;	import app.services.FacebookService;	import org.robotlegs.mvcs.Command;	/**	 * @author marco link (m.link@gmx.de)	 */	public class FBLoginSuccessCommand extends Command	{		/*******************************************************************************************		*								public properties										   *		*******************************************************************************************/		[Inject]		public var service : FacebookService;		[Inject]		public var model : FBModel;						/*******************************************************************************************		*								protected/ private properties							   *		*******************************************************************************************/						/*******************************************************************************************		*								public methods											   *		*******************************************************************************************/		public function FBLoginSuccessCommand()		{					}				override public function execute() : void		{			model.setUserLoggedIn(true);			model.setUserData(service.getUserData());		}				/*******************************************************************************************		*								protected/ private methods								   *		*******************************************************************************************/	}}